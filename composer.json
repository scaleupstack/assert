{
    "name": "scaleupstack/assert",
    "description": "A tiny wrapper around webmozart/assert that is easily extendable to throw project-specific assertions.",
    "type": "library",
    "keywords": [
        "assert",
        "assertion",
        "check",
        "validation"
    ],
    "autoload": {
        "psr-4": {
            "ScaleUpStack\\Assert\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScaleUpStack\\Assert\\Tests\\": ["tests/phpunit/", "tests/resources/", "tests/fixtures/"]
        }
    },
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Thomas Nunninger",
            "email": "thomas.nunninger@scaleupventures.com"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "webmozart/assert": "^1."
    },
    "require-dev": {
        "phpunit/phpunit": "^7."
    }
}
